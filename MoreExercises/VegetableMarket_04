import java.util.Scanner;

public class VegetableMarket_04 {

    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);

        double priceN = Double.parseDouble(scanner.nextLine());
        double priceM = Double.parseDouble(scanner.nextLine());
        int kgN = Integer.parseInt(scanner.nextLine());
        int kgM = Integer.parseInt(scanner.nextLine());

        double N = kgN * priceN;
        double M = kgM * priceM;
        double sum = (N + M)/1.94;

        System.out.printf("%.2f", sum);
    }
}
